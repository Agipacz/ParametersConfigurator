<?xml version="1.0" encoding="UTF-8"?>

<pc:ParametersConfigurator
	xmlns:pc="http://www.czadzik.pl/parametersConfigurator" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.czadzik.pl/parametersConfigurator ../../parametersConfigurator.xsd">

	<pc:ParametersConfiguratorTitle>Condition Example 1</pc:ParametersConfiguratorTitle>
	<pc:Sections>
		<pc:Section>
			<pc:Parameters>
				<pc:DateTimeParameter maxNumberOfValues="1">
					<pc:Name>date1</pc:Name>
				</pc:DateTimeParameter>
			</pc:Parameters>
		</pc:Section>
		<pc:Section>
			<pc:Parameters>
				<pc:IntegerParameter>
					<pc:Name>date1_Equalto_value_2017-11-19 20:00:00</pc:Name>
				</pc:IntegerParameter>
			</pc:Parameters>
			<pc:VisibilityCondition>
					<pc:FirstParameterName>date1</pc:FirstParameterName>
					<pc:Clause>==</pc:Clause>
					<pc:Value>2017-11-19 20:00:00</pc:Value>
			</pc:VisibilityCondition>
		</pc:Section>
		<pc:Section>
			<pc:Parameters>
				<pc:IntegerParameter>
					<pc:Name>date1_notEqualto_value_2017-11-19 21:00:00</pc:Name>
				</pc:IntegerParameter>
			</pc:Parameters>
			<pc:VisibilityCondition>
					<pc:FirstParameterName>date1</pc:FirstParameterName>
					<pc:Clause>!=</pc:Clause>
					<pc:Value>2017-11-19 21:00:00</pc:Value>
			</pc:VisibilityCondition>
		</pc:Section>
		<pc:Section>
			<pc:Parameters>
				<pc:IntegerParameter>
					<pc:Name>date1_GreaterThan_value_2017/11/19 18:00:00</pc:Name>
				</pc:IntegerParameter>
			</pc:Parameters>
			<pc:VisibilityCondition>
					<pc:FirstParameterName>date1</pc:FirstParameterName>
					<pc:Clause>GreaterThan</pc:Clause>
					<pc:Value>2017-11-19 18:00:00</pc:Value>
			</pc:VisibilityCondition>
		</pc:Section>
		<pc:Section>
			<pc:Parameters>
				<pc:IntegerParameter>
					<pc:Name>date1_GreaterEqual_value_2017/11/19 17:00:00</pc:Name>
				</pc:IntegerParameter>
			</pc:Parameters>
			<pc:VisibilityCondition>
					<pc:FirstParameterName>date1</pc:FirstParameterName>
					<pc:Clause>>=</pc:Clause>
					<pc:Value>2017-11-19 17:00:00</pc:Value>
			</pc:VisibilityCondition>
		</pc:Section>
		<pc:Section>
			<pc:Parameters>
				<pc:IntegerParameter>
					<pc:Name>date1_LessThan_value_2017/11/19 18:00:00</pc:Name>
				</pc:IntegerParameter>
			</pc:Parameters>
			<pc:VisibilityCondition>
					<pc:FirstParameterName>date1</pc:FirstParameterName>
					<pc:Clause>LessThan</pc:Clause>
					<pc:Value>2017-11-19 18:00:00</pc:Value>
			</pc:VisibilityCondition>
		</pc:Section>
		<pc:Section>
			<pc:Parameters>
				<pc:IntegerParameter>
					<pc:Name>date1_LessEqual_value_2017/11/19 19:00:00</pc:Name>
				</pc:IntegerParameter>
			</pc:Parameters>
			<pc:VisibilityCondition>
					<pc:FirstParameterName>date1</pc:FirstParameterName>
					<pc:Clause>LessOrEqual</pc:Clause>
					<pc:Value>2017-11-19 19:00:00</pc:Value>
			</pc:VisibilityCondition>
		</pc:Section>
	</pc:Sections>
</pc:ParametersConfigurator>