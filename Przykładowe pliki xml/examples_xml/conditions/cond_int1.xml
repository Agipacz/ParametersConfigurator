<?xml version="1.0" encoding="UTF-8"?>

<pc:ParametersConfigurator
	xmlns:pc="http://www.czadzik.pl/parametersConfigurator" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.czadzik.pl/parametersConfigurator ../../parametersConfigurator.xsd">

	<pc:ParametersConfiguratorTitle>Condition Example Int 1</pc:ParametersConfiguratorTitle>
	<pc:Sections>
		<pc:Section>
			<pc:Parameters>
				<pc:IntegerParameter maxNumberOfValues="1">
					<pc:Name>int1</pc:Name>
				</pc:IntegerParameter>
			</pc:Parameters>
		</pc:Section>
		<pc:Section>
			<pc:Parameters>
				<pc:IntegerParameter>
					<pc:Name>int1_Equalto_value_100</pc:Name>
				</pc:IntegerParameter>
			</pc:Parameters>
			<pc:VisibilityCondition>
					<pc:FirstParameterName>int1</pc:FirstParameterName>
					<pc:Clause>==</pc:Clause>
					<pc:Value>100</pc:Value>
			</pc:VisibilityCondition>
		</pc:Section>
		<pc:Section>
			<pc:Parameters>
				<pc:IntegerParameter>
					<pc:Name>int1_notEqualto_value_50</pc:Name>
				</pc:IntegerParameter>
			</pc:Parameters>
			<pc:VisibilityCondition>
					<pc:FirstParameterName>int1</pc:FirstParameterName>
					<pc:Clause>!=</pc:Clause>
					<pc:Set>
						<pc:Item>A</pc:Item>
					</pc:Set>
			</pc:VisibilityCondition>
		</pc:Section>
		<pc:Section>
			<pc:Parameters>
				<pc:IntegerParameter>
					<pc:Name>int1_GreaterThan_value_150</pc:Name>
				</pc:IntegerParameter>
			</pc:Parameters>
			<pc:VisibilityCondition>
					<pc:FirstParameterName>int1</pc:FirstParameterName>
					<pc:Clause>GreaterThan</pc:Clause>
					<pc:Value>150</pc:Value>
			</pc:VisibilityCondition>
		</pc:Section>
		<pc:Section>
			<pc:Parameters>
				<pc:IntegerParameter>
					<pc:Name>int1_GreaterEqual_value_120</pc:Name>
				</pc:IntegerParameter>
			</pc:Parameters>
			<pc:VisibilityCondition>
					<pc:FirstParameterName>int1</pc:FirstParameterName>
					<pc:Clause>>=</pc:Clause>
					<pc:Value>120</pc:Value>
			</pc:VisibilityCondition>
		</pc:Section>
		<pc:Section>
			<pc:Parameters>
				<pc:IntegerParameter>
					<pc:Name>int1_LessThan_value_12.5</pc:Name>
				</pc:IntegerParameter>
			</pc:Parameters>
			<pc:VisibilityCondition>
					<pc:FirstParameterName>int1</pc:FirstParameterName>
					<pc:Clause>LessThan</pc:Clause>
					<pc:Value>12.5</pc:Value>
			</pc:VisibilityCondition>
		</pc:Section>
		<pc:Section>
			<pc:Parameters>
				<pc:IntegerParameter>
					<pc:Name>int1_LessEqual_value_-35</pc:Name>
				</pc:IntegerParameter>
			</pc:Parameters>
			<pc:VisibilityCondition>
					<pc:FirstParameterName>int1</pc:FirstParameterName>
					<pc:Clause>LessOrEqual</pc:Clause>
					<pc:Value>-35</pc:Value>
			</pc:VisibilityCondition>
		</pc:Section>
		<pc:Section>
			<pc:Parameters>
				<pc:IntegerParameter>
					<pc:Name>int1_IsIn_value_56</pc:Name>
				</pc:IntegerParameter>
			</pc:Parameters>
			<pc:VisibilityCondition>
					<pc:FirstParameterName>int1</pc:FirstParameterName>
					<pc:Clause>IsSubset</pc:Clause>
					<pc:Value>56</pc:Value>
			</pc:VisibilityCondition>
		</pc:Section>
	</pc:Sections>
</pc:ParametersConfigurator>